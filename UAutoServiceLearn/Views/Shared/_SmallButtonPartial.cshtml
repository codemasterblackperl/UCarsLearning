@model UAutoServiceLearn.Models.SmallButtonPartial

    <a class="btn btn-sm @Model.ButtonStyleType" type="button"
       href="@Url.Action(Model.ActionName)@Model.ActionParameters"
       >
        <span class="glyphicon glyphicon-@Model.GlyphIcon"></span>
        <span class="sr-only">@Model.Text</span>
    </a>

