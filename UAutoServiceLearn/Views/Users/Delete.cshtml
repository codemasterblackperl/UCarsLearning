@model UAutoServiceLearn.Models.ApplicationUser
@{
    ViewData["Title"] = "Delete";
}

<h2>Delete a user</h2>
<br />

<form asp-action="Delete">
    <div asp-validation-summary="All" class="text-danger"></div>
    <div class="form-border">
        <fieldset disabled>
            <div class="form-group">
                <div class="row">
                    <div class="col-sm-1">
                        <label asp-for="FirstName" class="custom-label"></label>
                    </div>
                    <div class="col-sm-4">
                        <input asp-for="FirstName" class="form-control" />
                    </div>
                </div>
            </div>
            <div class="form-group">
                <div class="row">
                    <div class="col-sm-1">
                        <label asp-for="LastName" class="custom-label"></label>
                    </div>
                    <div class="col-sm-4">
                        <input asp-for="LastName" class="form-control" />
                    </div>
                </div>
            </div>
            <div class="form-group">
                <div class="row">
                    <div class="col-sm-1">
                        <label asp-for="Email" class="custom-label"></label>
                    </div>
                    <div class="col-sm-4">
                        <input asp-for="Email" class="form-control" />
                    </div>
                </div>
            </div>
            <div class="form-group">
                <div class="row">
                    <div class="col-sm-1">
                        <label asp-for="PhoneNumber" class="custom-label"></label>
                    </div>
                    <div class="col-sm-4">
                        <input asp-for="PhoneNumber" class="form-control" />
                    </div>
                </div>
            </div>
            <div class="form-group">
                <div class="row">
                    <div class="col-sm-1">
                        <label asp-for="Address" class="custom-label"></label>
                    </div>
                    <div class="col-sm-4">
                        <input asp-for="Address" class="form-control" />
                    </div>
                </div>
            </div>
            <div class="form-group">
                <div class="row">
                    <div class="col-sm-1">
                        <label asp-for="City" class="custom-label"></label>
                    </div>
                    <div class="col-sm-4">
                        <input asp-for="City" class="form-control" />
                    </div>
                </div>
            </div>
            <div class="form-group">
                <div class="row">
                    <div class="col-sm-1">
                        <label asp-for="PostalCode" class="custom-label"></label>
                    </div>
                    <div class="col-sm-4">
                        <input asp-for="PostalCode" class="form-control" />
                    </div>
                </div>
            </div>
        </fieldset>

        <div class="form-group">
            <input type="submit" class="btn btn-sm btn-danger" value="Delete" />
            <a class="btn btn-sm btn-success" asp-action="Index">Back</a>
        </div>
    </div>
</form>
