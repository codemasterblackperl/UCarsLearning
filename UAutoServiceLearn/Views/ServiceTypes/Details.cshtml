@model UAutoServiceLearn.Models.ServiceType
@{
    ViewData["Title"] = "Details";
}
<br />
<h2>Service type details</h2>
<br />

<div class="form-border">
    <div class="form-group">
        <div class="row">
            <div class="col-sm-1">
                <label asp-for="Name" class="custom-label"></label>
            </div>
            <div class="col-sm-4">
                <input asp-for="Name" disabled class="form-control" />
            </div>
        </div>
    </div>
    <div class="form-group">
        <a asp-action="Edit" asp-route-id="@Model.Id" class="btn btn-sm btn-primary">Edit</a>
        <a class="btn btn-sm btn-success" asp-action="Index">Back</a>
    </div>
</div>
