@model UAutoServiceLearn.Models.Car
@{
    ViewData["Title"] = "Delete";
}

<h2>Edit</h2>
<br />

<div class="form-border">
    <form asp-action="Delete">
        <fieldset disabled>
            <input type="hidden" asp-for="UserId" />
            <div class="form-group">
                <div class="row">
                    <div class="col-sm-1">
                        <label asp-for="VIN" class="custom-label"></label>
                    </div>
                    <div class="col-sm-4">
                        <input asp-for="VIN" class="form-control" />
                    </div>
                </div>
            </div>
            <div class="form-group">
                <div class="row">
                    <div class="col-sm-1">
                        <label asp-for="Make" class="custom-label"></label>
                    </div>
                    <div class="col-sm-4">
                        <input asp-for="Make" class="form-control" />
                    </div>
                </div>
            </div>
            <div class="form-group">
                <div class="row">
                    <div class="col-sm-1">
                        <label asp-for="Model" class="custom-label"></label>
                    </div>
                    <div class="col-sm-4">
                        <input asp-for="Model" class="form-control" />
                    </div>
                </div>
            </div>
            <div class="form-group">
                <div class="row">
                    <div class="col-sm-1">
                        <label asp-for="Style" class="custom-label"></label>
                    </div>
                    <div class="col-sm-4">
                        <input asp-for="Style" class="form-control" />
                    </div>
                </div>
            </div>
            <div class="form-group">
                <div class="row">
                    <div class="col-sm-1">
                        <label asp-for="Year" class="custom-label"></label>
                    </div>
                    <div class="col-sm-4">
                        <input asp-for="Year" class="form-control" />
                    </div>
                </div>
            </div>
            <div class="form-group">
                <div class="row">
                    <div class="col-sm-1">
                        <label asp-for="Miles" class="custom-label"></label>
                    </div>
                    <div class="col-sm-4">
                        <input asp-for="Miles" class="form-control" />
                    </div>
                </div>
            </div>
            <div class="form-group">
                <div class="row">
                    <div class="col-sm-1">
                        <label asp-for="Color" class="custom-label"></label>
                    </div>
                    <div class="col-sm-4">
                        <input asp-for="Color" class="form-control" />
                    </div>
                </div>
            </div>
        </fieldset>
        <div class="form-group">
            <input type="submit" class="btn btn-sm btn-danger" value="Delete" />
            <a class="btn btn-sm btn-success" asp-action="Index" asp-route-id="@Model.UserId">Back</a>
        </div>
    </form>
</div>

